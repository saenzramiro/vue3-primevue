{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Server",
			"port": 9229,
			"restart": true,
			"skipFiles": ["<node_internals>/**"],
			"preLaunchTask": "Back-End"
		}
	]
}
