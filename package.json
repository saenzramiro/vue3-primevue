{
	"name": "vue3-primevue",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview --mode staging",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
		"format": "prettier .  --write",
		"deploy": "firebase deploy --only hosting",
		"server": "npm run build:watch --prefix api | npm run emulator",
		"emulator": "firebase emulators:start --export-on-exit=local --import=local --inspect-functions"
	},
	"dependencies": {
		"@primevue/themes": "4.2.1",
		"@xterm/addon-attach": "0.11.0",
		"@xterm/addon-fit": "0.10.0",
		"@xterm/xterm": "5.5.0",
		"chart.js": "4.4.3",
		"firebase": "10.12.1",
		"firebaseui": "6.1.0",
		"gridstack": "10.3.1",
		"pinia": "2.1.7",
		"pinia-plugin-persistedstate": "3.2.1",
		"primeicons": "7.0.0",
		"primevue": "4.2.1",
		"tailwindcss-primeui": "0.3.4",
		"vue": "3.4.27",
		"vue-router": "4.4.0",
		"vuedraggable": "4.1.0",
		"vuefire": "3.1.23"
	},
	"devDependencies": {
		"@primevue/auto-import-resolver": "4.2.1",
		"@types/node": "20.10.5",
		"@typescript-eslint/parser": "6.16.0",
		"@vite-pwa/assets-generator": "0.2.4",
		"@vitejs/plugin-vue": "5.1.2",
		"autoprefixer": "10.4.20",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-vue": "9.19.2",
		"firebase-tools": "13.10.0",
		"postcss": "8.4.47",
		"prettier": "3.1.1",
		"sass": "1.77.6",
		"tailwindcss": "3.4.14",
		"typescript": "5.2.2",
		"unplugin-vue-components": "0.27.3",
		"vite": "5.4.0",
		"vite-plugin-eslint": "1.8.1",
		"vite-plugin-pwa": "0.20.1",
		"vite-plugin-vue-devtools": "7.4.5",
		"vue-tsc": "1.8.25"
	},
	"volta": {
		"node": "20.14.0"
	}
}
