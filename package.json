{
	"name": "vue3-primevue",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview --mode staging",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
		"format": "prettier .  --write",
		"deploy": "firebase deploy --only hosting",
		"server": "npm run build:watch --prefix api | npm run emulator",
		"emulator": "firebase emulators:start --export-on-exit=local --import=local --inspect-functions"
	},
	"dependencies": {
		"@primeuix/themes": "1.1.1",
		"chart.js": "4.5.0",
		"firebase": "11.9.1",
		"pinia": "3.0.3",
		"pinia-plugin-persistedstate": "4.3.0",
		"primeicons": "7.0.0",
		"primevue": "4.3.5",
		"tailwindcss-primeui": "0.6.1",
		"vue": "3.5.16",
		"vue-router": "4.5.1",
		"vuefire": "3.2.1"
	},
	"devDependencies": {
		"@primevue/auto-import-resolver": "4.3.5",
		"@types/node": "20.10.5",
		"@typescript-eslint/parser": "6.16.0",
		"@vitejs/plugin-vue": "5.1.2",
		"autoprefixer": "10.4.20",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-vue": "9.19.2",
		"firebase-tools": "14.7.0",
		"postcss": "8.4.47",
		"prettier": "3.1.1",
		"sass": "1.77.6",
		"tailwindcss": "3.4.14",
		"typescript": "5.8.3",
		"unplugin-vue-components": "28.7.0",
		"vite": "5.4.0",
		"vite-plugin-eslint": "1.8.1",
		"vite-plugin-vue-devtools": "7.4.5",
		"vue-tsc": "1.8.25"
	},
	"volta": {
		"node": "20.14.0"
	}
}
